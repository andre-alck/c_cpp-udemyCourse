#include <iostream>
#include <string>

class Person {
private:
    int age;
    std::string fullName;

public:
    Person();
    Person(int newAge);
    Person(int newAge, std::string newFullName);
};

Person::Person() {
    age = 0;
    fullName = "undefined";
}

Person::Person(int newAge) {
    age = newAge;
    fullName = "undefined";
}

Person::Person(int newAge, std::string newFullName) {
    age = newAge;
    fullName = newFullName;
}

int main(void) {
    Person gabriel();
    Person pedro(10);
    Person maria(21, "Maria Flamingo");

    return 0;
}
