#include <iostream>
#include <string>
#include "person.h"

Person::Person() {
    std::cout << "Object created." << std::endl;
}

Person::~Person() {
    std::cout << "Object destroyed." << std::endl;
}

std::string Person::getFavoriteGame (void) {
    return favoriteGame;
}

void Person::setFavoriteGame (std::string newFavoriteGame) {
    favoriteGame = newFavoriteGame;
}
